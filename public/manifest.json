{
    "manifest_version": 2,
    "name": "Your Chrome Extension Name",
    "version": "1.0",
    "description": "Description of your Chrome extension",
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "*://*.linkedin.com/*" 
      ],
      "content_scripts": [
        {
          "matches": ["*://*.linkedin.com/*"],
          "js": ["content.js"]
        }
      ],
    "browser_action": {
        "default_popup": "popup.html"
    }
}